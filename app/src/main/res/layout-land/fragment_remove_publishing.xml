<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context="johnschroeders.marketfree.RemovePublishingFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="730dp"
        android:layout_height="380dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editPubXButton">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout10"
            android:layout_width="728dp"
            android:layout_height="214dp"
            app:layout_constraintBottom_toTopOf="@+id/editPubProductDescrInput"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout8"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/constraintLayout9"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:id="@+id/editPubProductImageView"
                    android:layout_width="156dp"
                    android:layout_height="155dp"
                    android:contentDescription="@string/publsihingfragonclickimageview"
                    android:src="@drawable/bluebutton"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout9"
                android:layout_width="519dp"
                android:layout_height="213dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/constraintLayout8"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/editPubProductIDLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:text="@string/product_idplaceholder"
                    app:layout_constraintBottom_toTopOf="@+id/editPubProductCostLabel"
                    app:layout_constraintEnd_toStartOf="@+id/editPubProductIDInput"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:text="@string/product_idplaceholder" />

                <TextView
                    android:id="@+id/editPubProductDateCreatedInput"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="@string/NA"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/editPubProductDateCreatedLabel"
                    app:layout_constraintTop_toBottomOf="@+id/editPubProductQuantityInput" />

                <TextView
                    android:id="@+id/editPubProductQuantityInput"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="@string/NA"
                    app:layout_constraintBottom_toTopOf="@+id/editPubProductDateCreatedLabel"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/editPubProductQuantityLabel"
                    app:layout_constraintTop_toBottomOf="@+id/editPubProductCostInput" />

                <TextView
                    android:id="@+id/editPubProductDateCreatedLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:text="@string/prodDateCreated"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/editPubProductDateCreatedInput"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/editPubProductQuantityInput" />

                <TextView
                    android:id="@+id/editPubProductQuantityLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    app:layout_constraintBottom_toTopOf="@+id/editPubProductDateCreatedLabel"
                    app:layout_constraintEnd_toStartOf="@+id/editPubProductQuantityInput"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/editPubProductCostInput"
                    tools:text="@string/quantity_label" />

                <TextView
                    android:id="@+id/editPubProductIDInput"
                    android:layout_width="0dp"
                    android:layout_height="20dp"
                    android:gravity="center"
                    android:text="@string/NA"
                    app:autoSizeStepGranularity="2sp"
                    app:layout_constraintBottom_toTopOf="@+id/editPubProductCostInput"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/editPubProductIDLabel"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/editPubProductCostLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:text="@string/cost_label"
                    app:layout_constraintBottom_toTopOf="@+id/editPubProductQuantityLabel"
                    app:layout_constraintEnd_toStartOf="@+id/editPubProductCostInput"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/editPubProductIDLabel" />

                <TextView
                    android:id="@+id/editPubProductCostInput"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="@string/NA"
                    app:layout_constraintBottom_toTopOf="@+id/editPubProductQuantityLabel"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/editPubProductCostLabel"
                    app:layout_constraintTop_toBottomOf="@+id/editPubProductIDInput" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/editPubProductDescrInput"
            android:layout_width="370dp"
            android:layout_height="147dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:gravity="center_horizontal|center_vertical"
            android:scrollHorizontally="false"
            android:text="@string/NA"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/editPubRemovePublishingButton"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout10"
            app:layout_constraintVertical_bias="1.0" />

        <Button
            android:id="@+id/editPubRemovePublishingButton"
            android:layout_width="243dp"
            android:layout_height="43dp"
            android:text="@string/remove_publishing_ButtonText"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/editPubProductDescrInput"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout10" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/editPubXButton"
        android:layout_width="42dp"
        android:layout_height="36dp"
        android:text="@string/xbuttonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/editPubProductTitleLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:scrollHorizontally="false"
        android:text="@string/product_title_label"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
