<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context="johnschroeders.marketfree.RemovePublishingFragment">

    <!-- TODO: Update blank fragment layout -->

    <Button
        android:id="@+id/editPubXButton"
        android:layout_width="43dp"
        android:layout_height="38dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:text="@string/xbuttonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/editPubProductIDLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="376dp"
        android:layout_marginTop="64dp"
        android:text="@string/product_idplaceholder"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="@string/product_idplaceholder" />

    <TextView
        android:id="@+id/editPubProductDateCreatedInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="480dp"
        android:layout_marginTop="204dp"
        android:text="@string/NA"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/editPubProductQuantityLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="388dp"
        android:layout_marginTop="152dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="@string/quantity_label" />

    <TextView
        android:id="@+id/editPubProductCostInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="476dp"
        android:layout_marginTop="108dp"
        android:text="@string/NA"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/editPubProductIDInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="476dp"
        android:layout_marginTop="64dp"
        android:text="@string/NA"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/editPubProductQuantityInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="476dp"
        android:layout_marginTop="152dp"
        android:text="@string/NA"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/editPubProductDateCreatedLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="364dp"
        android:layout_marginTop="204dp"
        android:text="@string/prodDateCreated"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/editPubProductCostLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="412dp"
        android:layout_marginTop="108dp"
        android:text="@string/cost_label"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/editPubProdDescriptionLabel"
        android:layout_width="214dp"
        android:layout_height="23dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="252dp"
        android:gravity="center_horizontal"
        android:scrollHorizontally="false"
        android:text="@string/product_description_label"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/editPubProductImageView"
        android:layout_width="217dp"
        android:layout_height="182dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="64dp"
        android:contentDescription="@string/publsihingfragonclickimageview"
        android:src="@drawable/bluebutton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/editPubProductTitleLabel"
        android:layout_width="98dp"
        android:layout_height="40dp"
        android:layout_marginStart="156dp"
        android:layout_marginTop="14dp"
        android:gravity="center_horizontal"
        android:scrollHorizontally="false"
        android:text="@string/product_title_label"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/editPubRemovePublishingButton"
        android:layout_width="243dp"
        android:layout_height="43dp"
        android:layout_marginStart="464dp"
        android:layout_marginBottom="92dp"
        android:text="@string/remove_publishing_ButtonText"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/editPubProductDescrInput"
        android:layout_width="210dp"
        android:layout_height="122dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="275dp"
        android:gravity="center_horizontal|center_vertical"
        android:scrollHorizontally="false"
        android:text="@string/NA"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
