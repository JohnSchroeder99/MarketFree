<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/removePubFragmentContainer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context="johnschroeders.marketfree.RemovePublishingFragment">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/editPubRemovePublishingButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout4">

        <TextView
            android:id="@+id/editPubProductIDInput"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:gravity="center"
            android:scrollHorizontally="true"
            android:text="@string/NA"
            app:autoSizeStepGranularity="2sp"
            app:layout_constraintBottom_toTopOf="@+id/editPubProductCostInput"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/editPubProductIDLabel"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/editPubProductIDLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/product_idplaceholder"
            app:layout_constraintBottom_toTopOf="@+id/editPubProductCostLabel"
            app:layout_constraintStart_toStartOf="@+id/editPubProductCostLabel"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="@string/product_idplaceholder" />

        <TextView
            android:id="@+id/editPubProductCostLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/cost_label"
            app:layout_constraintBottom_toTopOf="@+id/editPubProductQuantityLabel"
            app:layout_constraintStart_toStartOf="@+id/editPubProductQuantityLabel"
            app:layout_constraintTop_toBottomOf="@+id/editPubProductIDLabel" />

        <TextView
            android:id="@+id/editPubProductCostInput"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/NA"
            app:layout_constraintBottom_toTopOf="@+id/editPubProductQuantityInput"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/editPubProductCostLabel"
            app:layout_constraintTop_toBottomOf="@+id/editPubProductIDInput" />

        <TextView
            android:id="@+id/editPubProductDateCreatedInput"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/NA"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/editPubProductDateCreatedLabel"
            app:layout_constraintTop_toBottomOf="@+id/editPubProductQuantityInput" />

        <TextView
            android:id="@+id/editPubProductQuantityLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/editPubQuantityLabel"
            app:layout_constraintBottom_toTopOf="@+id/editPubProductDateCreatedLabel"
            app:layout_constraintStart_toStartOf="@+id/editPubProductDateCreatedLabel"
            app:layout_constraintTop_toBottomOf="@+id/editPubProductCostLabel"
            tools:text="@string/quantity_label" />

        <TextView
            android:id="@+id/editPubProductQuantityInput"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/NA"
            app:layout_constraintBottom_toTopOf="@+id/editPubProductDateCreatedInput"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/editPubProductQuantityLabel"
            app:layout_constraintTop_toBottomOf="@+id/editPubProductCostInput" />

        <TextView
            android:id="@+id/editPubProductDateCreatedLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/prodDateCreated"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editPubProductQuantityLabel" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/editPubXButton"
        android:layout_width="43dp"
        android:layout_height="38dp"
        android:text="@string/xbuttonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/editPubProductImageView"
        android:layout_width="wrap_content"
        android:layout_height="170dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@string/publsihingfragonclickimageview"
        android:src="@drawable/bluebutton"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editPubProductTitleLabel" />

    <TextView
        android:id="@+id/editPubProductTitleLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:scrollHorizontally="false"
        android:text="@string/product_title_label"
        app:layout_constraintBottom_toTopOf="@+id/editPubProductImageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/editPubRemovePublishingButton"
        android:layout_width="243dp"
        android:layout_height="43dp"
        android:text="@string/remove_publishing_ButtonText"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editPubProductImageView">

        <TextView
            android:id="@+id/editPubProductDescrInput"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center_horizontal|center_vertical"
            android:scrollHorizontally="false"
            android:text="@string/NA"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
